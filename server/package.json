{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "db:create": "ts-node scripts/create-database.ts",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "db:migrate:test": "NODE_ENV=test ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "db:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
    "dev": "ts-node src/index.ts",
    "dev:test": "NODE_ENV=test ts-node src/index.ts",
    "start": "node dist/index.js",
    "test:integration": "NODE_ENV=test vitest run --config vitest.config.mts",
    "test:unit": "vitest run --config vitest.unit.config.mts",
    "test": "npm run test:unit && npm run test:integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.10",
    "@langchain/core": "^1.1.3",
    "@langchain/langgraph": "^1.0.4",
    "@langchain/openai": "^1.1.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.93.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "20.19.5"
  }
}
